{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "google"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "object-curly-spacing": 0,
    "require-jsdoc": 0,
    "max-len": 0
  }
}